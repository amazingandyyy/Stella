{
  "manifest_version": 2,

  "name": "Voice Web",
  "description": "A Launchpad app that browses the web using your voice",
  "version": "1.0",

  "browser_action": {
    "default_icon": "favicon-16x16.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "https://ajax.googleapis.com/"
  ]
}
